<Directory {{ http.path_to_packages_mirror }}>
Options Indexes FollowSymLinks
Allowoverride None
</Directory>

<Directory {{ path_to_tftp_files }}>
Options Indexes FollowSymLinks
Allowoverride None
</Directory>
		
Alias /{{ http.link_to_packages_mirror }} "{{ http.path_to_packages_mirror }}"
Alias /{{ link_to_tftp_files }} "{{ path_to_tftp_files }}"

